{
  "name": "tools-generate-survey-list",
  "version": "1.0.0",
  "description": "Generates recipient list for survey",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "node utils/runner.js",
    "check-duplicates": "node utils/remove-duplicates.js",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "0.18.0",
    "birthdate-from-id": "2.0.0",
    "csvtojson": "2.0.8",
    "dotenv": "6.2.0",
    "get-age": "1.0.1",
    "jsonwebtoken": "8.4.0",
    "then-sleep": "1.0.1",
    "uuid-random": "1.0.6"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.2",
    "husky": "1.2.1",
    "nyc": "13.1.0",
    "standard": "12.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test-offline"
    }
  }
}
